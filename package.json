{
  "name": "Graphene-UI",
  "version": "1.1.0",
  "description": "Advanced wallet interface for graphene based blockchains (Bishares, Muse..)",
  "homepage": "https://github.com/cryptonomex/graphene_ui",
  "author": "Cryptonomex, Inc.",
  "license": "LicenseRef-LICENSE",
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cryptonomex/graphene_ui"
  },
  "scripts": {
    "test": "test",
    "profile-dev": "webpack --config web/webpack.config.js --profile --json > stats-dev.json",
    "profile-build": "NODE_ENV=production webpack --config web/webpack.config.js --profile --json > stats-prod.json",
    "start": "node server.js",
    "build": "cd web; NODE_ENV=production webpack --config webpack.config.js --progress --display-error-details --colors",
    "electron": "NODE_ENV=production webpack --config web/webpack.config.js --progress --display-error-details --colors;",
    "postelectron": " echo 'copying to ./electron/build..'; cp -r web/dist/* ./electron/build/; echo 'done.'",
    "build-win32": "set NODE_ENV=production && webpack --config web/webpack.config.js --progress --colors &&  echo 'copying to electron/build..' &&  xcopy dist ..\\electron\\build /s /e /y && echo 'done.'"
  },
